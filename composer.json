{
  "name": "UltimateHuskiesWebsite",
  "type": "Website",
  "license": "MIT",
  "description": "A modern WordPress site for the ultimate frisbee team Ultime Huskies",
  "homepage": "http://ultimate-huskies.de/",
  "authors": [
    {
      "name": "Patrick MÃ¼ller",
      "email": "dev@mupat.net",
      "homepage": "http://mupat.net"
    }
  ],
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.4.0",
    "wpackagist-plugin/bbPress": "^2.5",
    "wpackagist-plugin/bbpress-pencil-unread": "^1.0",
    "wpackagist-plugin/gd-bbpress-attachments": "^2.3.1",
    "jarednova/timber": "^0.21.6",
    "wpackagist-plugin/google-calendar-events": "^3.0.8"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin", "jarednova/timber"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
