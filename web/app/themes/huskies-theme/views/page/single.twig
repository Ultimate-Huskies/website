{% extends "layout/base.twig" %}

{% block header %}
  <div class="slideshow slideshow--no-images">
    <h1 class="slideshow__title">{{ page.title }}</h1>
  </div>
{% endblock %}

{% block content %}
<section class="section">
  <article class="post-full {{ page.class }}">
    {{ page.content }}
  </article>
</section>
{% endblock %}
