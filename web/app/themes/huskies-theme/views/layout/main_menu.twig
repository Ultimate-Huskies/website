<div class="main_menu__overlay"></div>
<div class="main_menu">
  <a href="{{ site.url }}" class="main_menu__logo">
    <svg>
      <use xlink:href="#logo-font" width="60%" height="60%" x="21.5%" y="41.5%"></use>
      <use class="main_menu__logo--husky" xlink:href="#logo-husky" width="90%" height="90%" x="-14%" y="0%"></use>
    </svg>
  </a>
  <ul class="main_menu__items">
    <li class="main_menu__item main_menu__item--seperate">
      <a class="main_menu__link" href="{{ function('wp_login_url', function('get_permalink')) }}"> {{ __('Login', 'huskies') }} </a>
    </li>
    {% for item in menu.get_items %}
      <li class="main_menu__item {{ item.classes | join(' ') }}">
        <a class="main_menu__link" href="{{ item.get_link }}">{{ item.title }}</a>
      </li>
    {% endfor %}
  </ul>
</div>
